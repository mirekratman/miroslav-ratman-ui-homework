{
  "compilerOptions": {
    "noEmit": true,
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@keboola/admin/*": ["./src/admin/*"],
      "@keboola/shared/*": ["./src/shared/*"],
      "@keboola/components/*": ["./src/components/*"]
    }
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
